   "source": [
    "def hailstone(i):\n",
    "    step = 1\n",
    "    while i>1:\n",
    "        i = i/2 if i%2==0 else i*3+1\n",
    "        step+=1\n",
    "    return step\n",
    "    \n",
    "print(hailstone(27))"
   ]
